{
  "name": "npg-ui",
  "version": "0.1.0",
  "private": true,
  "description": "Network PlayGround UI",
  "scripts": {
    "serve": "VUE_APP_SOCKET=1 vue-cli-service serve --open",
    "dev": "vue-cli-service serve --open",
    "build": "VUE_APP_SOCKET=1 vue-cli-service build",
    "build:stats": "webpack --config node_modules/@vue/cli-service/webpack.config.js --mode production --profile --json > .tmp; sed '/^{/,$!d' .tmp > stats.json; rm .tmp",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit --coverage --verbose",
    "test:e2e": "vue-cli-service test:e2e --headless",
    "dep": "npm-check -s"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/vue-fontawesome": "0.0.22",
    "bulma": "^0.7.1",
    "socket.io-client": "^2.1.1",
    "split.js": "^1.3.5",
    "v-tooltip": "^2.0.0-rc.32",
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.1.0",
    "vue-resize": "^0.4.4",
    "vuex": "^3.0.1",
    "xterm": "^3.4.1"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/socket.io-client": "^1.4.32",
    "@types/split.js": "^1.3.1",
    "@vue/cli-plugin-babel": "^3.0.0-beta.15",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.15",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.15",
    "@vue/cli-plugin-unit-jest": "^3.0.0-beta.15",
    "@vue/cli-service": "^3.0.0-beta.15",
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "node-sass": "^4.9.0",
    "npm-check": "^5.7.1",
    "sass-loader": "^7.0.1",
    "ts-jest": "^22.4.6",
    "ts-vue-plugin": "^0.2.0",
    "vue-template-compiler": "^2.5.16",
    "webpack-cli": "^2.1.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
